#VRML V2.0 utf8

Shape{
	appearance Appearance{
		material Material{
			diffuseColor 1 0 0
		}
	}
	geometry Sphere{radius 1}
}

Transform{
	translation 0 -.45 0
	children[
		Shape{		
			appearance Appearance{
				material Material{
					diffuseColor 1 1 1
				}
			}
		geometry Sphere{radius 1}
		}
	]
}